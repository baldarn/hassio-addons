{
  "arch": [
    "aarch64",
    "amd64"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "Cryptpad for Home Assistant",
  "image": "ghcr.io/alexbelgium/cryptpad-{arch}",
  "init": false,
  "map": [
    "share:rw",
    "ssl:rw"
  ],
  "name": "Cryptpad",
  "options": {
    "DOMAIN": "homeassistant.local",
    "SANDBOX_DOMAIN": "homeassistant.local",
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "ports": {
    "3000/tcp": 3000,
    "3001/tcp": 3001
  },
  "ports_description": {
    "3000/tcp": "Web interface",
    "3001/tcp": "Safe Web interface"
  },
  "schema": {
    "APP_NAME": "str?",
    "DOMAIN": "str?",
    "ROOT_URL": "str?",
    "certfile": "str",
    "keyfile": "str",
    "ssl": "bool"
  },
  "slug": "cryptpad",
  "url": "https://github.com/alexbelgium/hassio-addons/tree/master/cryptpad",
  "version": "5.2.1",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:3000]"
}
